@model IEnumerable<Project.Web.BlogEntryViewModel>
@{
    ViewBag.Title = "Блог";
}

<h2>Index</h2>

@if (User.Identity.IsAuthenticated)
{
    @Html.ActionLink("Add Blog Entry", "Create", null, new { @class = "btn btn-success" })
}

@foreach (var item in Model)
{
    @Html.Partial("_BlogEntryPartial", item);
}